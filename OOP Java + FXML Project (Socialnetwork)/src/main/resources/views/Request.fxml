   <children>
      <Button fx:id="buttonAccept" layoutX="380.0" layoutY="77.0" mnemonicParsing="false" onAction="#onAcceptRequest" prefHeight="50.0" prefWidth="130.0" style="-fx-background-color: pink;" text="Accept Request">
